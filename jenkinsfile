pipeline {
//None parameter in the agent section means that no global agent will be allocated for the entire Pipelineâ€™s
//execution and that each stage directive must specify its own agent section.
    agent any
    stages {
        stage('Build') {
            steps {
            sh 'pwd'
            dir('src'){
            sh 'pwd'
            sh 'ls'
            }
            sh 'ls'
            sh 'python server.py'
            }
        }
    }
}
